<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:status="clr-namespace:DigitalRune.Editor.Status">

    <DataTemplate DataType="{x:Type status:StatusConductor}">
        <ContentControl Content="{Binding ActiveItem}" />
    </DataTemplate>

    <DataTemplate DataType="{x:Type status:StatusViewModel}">
        <status:StatusView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type status:TotalMemoryViewModel}">
        <status:TotalMemoryView />
    </DataTemplate>

</ResourceDictionary>
